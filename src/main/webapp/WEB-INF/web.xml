<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>FrontController</servlet-name>
  	<display-name>FrontController</display-name>
  	<description></description>
  	<servlet-class>ikujo.frontcontroller.FrontController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>NutrientsAjax</servlet-name>
  	<display-name>NutrientsAjax</display-name>
  	<description></description>
  	<servlet-class>ikujo.controller.NutrientsAjax</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RecommendSys</servlet-name>
  	<display-name>RecommendSys</display-name>
  	<description></description>
  	<servlet-class>ikujo.controller.RecommendSys</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RecommendFoodInsert</servlet-name>
  	<display-name>RecommendFoodInsert</display-name>
  	<description></description>
  	<servlet-class>ikujo.controller.RecommendFoodInsert</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DessertAjax</servlet-name>
  	<display-name>DessertAjax</display-name>
  	<description></description>
  	<servlet-class>ikujo.controller.DessertAjax</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>FrontController</servlet-name>
  	<url-pattern>*.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>NutrientsAjax</servlet-name>
  	<url-pattern>/NutrientsAjax</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RecommendSys</servlet-name>
  	<url-pattern>/RecommendSys</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RecommendFoodInsert</servlet-name>
  	<url-pattern>/RecommendFoodInsert</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DessertAjax</servlet-name>
  	<url-pattern>/DessertAjax</url-pattern>
  </servlet-mapping>
</web-app>
