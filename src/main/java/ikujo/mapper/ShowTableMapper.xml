<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ikujo.mapper.ShowTableMapper">

	
	<select id="showFoodId" parameterType="String" resultType="ShowFoodDTO">
		select u.udate,f.* from food f, userui u where f.foodid=u.foodid and u.userid =#{id}
	</select>
	
	<select id="showNutrients" parameterType="String" resultType="NutrientsDTO">
		select * from nutrients where nutnm=#{nutnm}
	</select>
	
	<select id="showNutImg" parameterType="String" resultType="String">
		select img from nutrients where nutnm=#{nutnm}
	</select>
</mapper>